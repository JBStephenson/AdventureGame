<?xml version="1.0" encoding="utf-8"?>
<room name="room1" points="0" description="Starting Room">
    <path command="up" dest="room2"/>
    <item name="ax" points=3/>
</room>
<room name="room2" points="1" description="Room with snake">
    <path command="down" dest="room1"/>
